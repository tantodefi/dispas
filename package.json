    "remove:contract": "yarn workspace @se-2/hardhat remove",
    "format:frontend": "yarn workspace @se-2/nextjs format",
    "format:contract": "yarn workspace @se-2/hardhat format"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "@lukso/lsp-smart-contracts": "0.10.2",
    "@openzeppelin/contracts": "4.7.3",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "next": "^15.1.7"
  }
}
